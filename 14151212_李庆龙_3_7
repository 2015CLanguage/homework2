# include<stdio.h>
int main()
{
	float r,h,l,s1,s2,v1,v2;
	printf("请输入圆半径：");
	scanf("%f",&r);
	printf("请输入圆柱高：");
	scanf("%f",&h);
	l=(float)(2*r*3.14);
	s1=(float)(3.4*r*r);
	s2=(float)(4*3.14*r*r);
	v1=(float)((float)4/3*3.14*r*r*r);
	v2=(float)(s1*h);
	printf("%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n",l,s1,s2,v1,v2);
	return 0;
}
